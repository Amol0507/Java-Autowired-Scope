<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="lap" class="com.model.Laptop" autowire="byName" >
<property name="id" value="101"> </property>  
<property name="name" value="Lenovo"> </property>

</bean>


<bean id="harddisk" class="com.model.Harddisk"  >
<property name="hid" value="1001"> </property>  
<property name="hname" value="pata"> </property>

</bean>
 </beans>
 



